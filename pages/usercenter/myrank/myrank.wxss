/* pages/usercenter/myrank/myrank.wxss */
page{
  min-height: 100%;
  background: #027CE5;
}
.container{
  padding: 0;
}
.head-mian{
  height: 124rpx;
  display: flex;
  padding: 36rpx 30rpx 36rpx 50rpx;
  overflow: hidden;
  position: relative;
  background: #1C96FF}
.head-mian .pic{
  width: 124rpx;
  height: 124rpx;
  overflow: hidden;
  background: url('data:image/png;base64,');
  background-size: 100% 100%
}
.head-mian image{
  width: 124rpx;
  height: 124rpx;
  border-radius: 50%;
}
.head-info{
  height: 100rpx;
  overflow: hidden;
  padding-left: 30rpx;
  padding-top: 22rpx;
}
.head-info text{
display: block;
color: #fff;
font-size: 24rpx;
line-height: 50rpx;
}
.head-info .username{
  font-weight: bold;
  font-size: 30rpx;
}
.has-study{
  position: absolute;
  right: 50rpx;
  bottom: 20rpx;
  color: #fff;
  font-size: 32rpx;
}
.has-study text{
  font-size: 76rpx;
}
.rank-tips{
  background: #FFF8E5;
  box-shadow: 0 2px 4px 8px #1982DC;
  border-radius: 22rpx;
  margin: 36rpx 30rpx 0 30rpx;
  position: relative;
  padding: 20rpx 0;
}
.rank-tips text{
  color: #DFA700;
  font-size: 28rpx;
  line-height: 50rpx;
  display: block;
  padding-left: 46rpx;
}
.rank-tips .close{
  position: absolute;
  right: 0;
  top: 0;
  color: #DFA700;
  font-size: 40rpx;
  width:60rpx;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
}
.rank-wrap{
  background: #FFFFFF;
  box-shadow: 0 6px 2px 8px #1982DC;
  border-radius: 22rpx;
  margin: 30rpx;
  overflow: hidden
}
.rank-tit{
  background: #EBF8FF;
  display: flex
}
.rank-tit text{
  color: #0282E7;
  font-size: 32rpx;
  flex: 1;
  text-align: center;
  height: 97rpx;
  line-height: 97rpx;
  font-weight: bold
}
.rank-list view{
  display: flex
}
.rank-list i,.rank-list text{
  flex: 1;
  text-align: center;
  color: #666;
  font-size: 32rpx;
  padding: 20rpx 0;
}
.rank-1 image{
  width: 50rpx;
}

