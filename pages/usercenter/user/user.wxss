.container{
  padding: 0;
}
.head-mian{
  height: 144rpx;
  display: flex;
  padding: 66rpx 30rpx 66rpx 80rpx;
  overflow: hidden;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
}
.head-mian .pic{
  width: 144rpx;
  height: 144rpx;
  overflow: hidden;
  background: url('data:image/png;base64,');
  background-size: 100% 100%
}
.head-mian image{
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
}
.head-info{
  height: 100rpx;
  overflow: hidden;
  padding-left: 30rpx;
  padding-top: 22rpx;
}
.head-info text{
display: block;
color: #fff;
font-size: 28rpx;
line-height: 50rpx;
}
.study-time{
  height: 200rpx;
  border-bottom: 20rpx solid #F2F2F2;
  overflow: hidden
}
.study-time .line{
  width:4rpx;
  height: 80rpx;
  background: #3A3A3A;
  margin: 60rpx 0 ;
}
.study-time .weui-cell__bd{
padding-top: 36rpx ;
}
.study-time text{
  display: block;
  font-size: 52rpx;
  color: #333
}
.study-time .tit{
  font-size: 24rpx;
  color: #666
}

.mini-btn-area .weui-cell{
  border-bottom: 1px solid #dfdfdf;
  padding: 0 30rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.mini-btn-area .arr{
  width: 20rpx;
  height: 20rpx;
  border-top: 1px solid #ccc;
  border-right: 1px solid #ccc;
  transform:rotate(45deg);
  -moz-transform:rotate(45deg);
  -webkit-transform:rotate(45deg);
  -o-transform:rotate(45deg);
  margin-top:40rpx;
}
.mini-btn-area button{
border: none;
height: 100%;
width: 100%;
border-radius: 0;
background: none;
}
.mini-btn-area .weui-cell:active{
background: #f0f0f0;
}
.mini-btn-area button:active, .mini-btn-area .button-hover{
background: none;
}










